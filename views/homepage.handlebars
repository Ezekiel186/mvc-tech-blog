{{#each postData as |posts| }}
<div class="row align-center mb-5">
  <div class="col-md-6">
    <h2>{{post.title}} {{post.user}}{{post.date}}</h2>
  </div>
  <div class="col-md-6">
    <p>{{post.description}}"</p>
  </div>
  <button id="comment">Comment</button>
</div>
{{/each}}

<script src="./js/home.js"></script>